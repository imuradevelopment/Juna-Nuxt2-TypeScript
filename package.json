    "@uppy/vue": "^0.4.3",
    "@uppy/webcam": "^2.0.4",
    "axios": "^1.0.0",
    "core-js": "^3.15.1",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
