{
  "name": "juna_nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev2": "nuxt --hostname 0.0.0.0 --port 3333",
    "debug": "node --inspect-brk=9229 ./node_modules/nuxt/bin/nuxt",
    "dev:api": "aspida --watch",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
